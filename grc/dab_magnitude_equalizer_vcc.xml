<?xml version="1.0"?>
<block>
  <name>Magnitude Equalizer</name>
  <key>dab_magnitude_equalizer_vcc</key>
  <category>[DAB]</category>
  <import>import dab</import>
  <make>dab.magnitude_equalizer_vcc($vlen, $num_symbols)</make>
  <param>
    <name>Number of carriers</name>
    <key>vlen</key>
    <type>int</type>
  </param>
  <param>
    <name>Symbols</name>
    <key>num_symbols</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$vlen</vlen>
  </sink>
  <sink>
    <name>trigger_in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>$vlen</vlen>
  </source>
  <source>
    <name>trigger out</name>
    <type>byte</type>
  </source>
</block>
