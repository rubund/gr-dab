  <param>
    <name>Service comp Lang</name>
    <key>service_comp_lang</key>
    <type>raw</type>
  </param>
  <param>
    <name>Protection mode</name>
    <key>protection_mode</key>
    <type>raw</type>
  </param>
  <param>
    <name>Data rate / 8kbit/s</name>
    <key>data_rate_n</key>
    <type>raw</type>
  </param>
  <param>
    <name>DAB+</name>
    <key>dabplus</key>
    <type>raw</type>
  </param>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
