    <key>num_subch</key>
    <type>int</type>
  </param>
  <param>
    <name>Ensemble label</name>
    <key>ensemble_label</key>
    <type>string</type>
  </param>
  <param>
    <name>Service label</name>
    <key>programme_service_labels</key>
