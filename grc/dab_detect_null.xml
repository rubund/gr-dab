<?xml version="1.0"?>
<block>
  <name>Detect NULL</name>
  <key>dab_detect_null</key>
  <category>[DAB]</category>
  <import>import dab</import>
  <make>dab.detect_null($length, False)</make>
  <param>
    <name>NULL Len. in samples</name>
    <key>length</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
