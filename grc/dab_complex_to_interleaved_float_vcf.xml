<block>
  <name>Complex to interleaved float</name>
  <key>dab_complex_to_interleaved_float_vcf</key>
  <category>[DAB]</category>
  <import>import dab</import>
  <make>dab.complex_to_interleaved_float_vcf($length)</make>
  <param>
    <name>Length</name>
    <key>length</key>
    <type>raw</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$length</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>float</type>
    <vlen>$length*2</vlen>
  </source>
</block>
